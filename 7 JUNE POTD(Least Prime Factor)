# GFG-DAILY-PROBLEM


class Solution {
 public:
  int prime(int n)
  {
      for(int i=2;i<=sqrt(n);i++)if(n%i==0)return i;
      return n;
  }
    vector<int> leastPrimeFactor(int n) {
        // code here
        vector<int> ans;
        for(int i=0;i<=n;i++)
            ans.push_back(prime(i));
        return ans;
    }
};
                             
                             
